package org.cts.emp;

import java.util.Scanner;

public class Employee {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner obj = new Scanner(System.in);
		
		System.out.println("please enter the empId value");
		int empId;
		empId =obj.nextInt();
		System.out.println("empId value is" +empId);
		
		
		System.out.println("please enter the empPhone value");
		long empPhone;
		empPhone =obj.nextLong();
		System.out.println("empPhone value is" +empPhone);
		
		System.out.println("please enter the empSalary value");
		float empSalary;
		empSalary = obj.nextFloat();
		System.out.println("empSalary vale is" +empSalary);
	
      System.out.println("please enter the empGendar");
      boolean empGendar;
      empGendar = obj.hasNext();
      System.out.println("empGendar value is" +empGendar);
	}

}
